<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<table-filter match-name="address" match-schema="public" />
	<table-filter match-name="baseentity" match-schema="public" />
	<table-filter match-name="event" match-schema="public" />
	<table-filter match-name="person" match-schema="public" />
	<table-filter match-name="teachertutor" match-schema="public" />
	<table schema="public" name="event"></table>
	<table schema="public" name="address"></table>
	<table schema="public" name="teachertutor"></table>
	<table schema="public" name="person"></table>
	<table schema="public" name="event_baseentity"></table>
	<table schema="public" name="baseentity"></table>
	<table schema="public" name="event">
		<column name="startdate" jdbc-type='' />
		<column name="enddate" jdbc-type='' />
		<column name="title" jdbc-type='' />
		<column name="comment" jdbc-type='' />
	</table>
	<table schema="public" name="address">
		<column name="street" jdbc-type='' />
		<column name="country" jdbc-type='' />
	</table>
	<table schema="public" name="teachertutor"></table>
	<table schema="public" name="person">
		<column name="lastname" jdbc-type='' />
		<column name="firstname" jdbc-type='' />
		<column name="birthdaydate" jdbc-type='' />
		<column name="birthdaycity" jdbc-type='' />
		<column name="email" jdbc-type='' />
		<column name="phone" jdbc-type='' />
		<column name="mobilephone" jdbc-type='' />
		<column name="fax" jdbc-type='' />
		<column name="address_fk" jdbc-type='' />
	</table>
	<table schema="public" name="baseentity">
		<column name="version" jdbc-type='' />
	</table>
</hibernate-reverse-engineering>